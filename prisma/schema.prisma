// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Patient {
  id            Int    @id
  first_Name    String
  last_Name     String
  Age           Int
  Email         String
  Phone         Int
  Date_of_Birth Int
  Address       String
  City          String
  Country       String
  Zip_Code      Int
  NHS_Number    String
}
